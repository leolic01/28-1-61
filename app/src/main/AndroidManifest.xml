<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".basic.BasicActivity"
            android:label="@string/basic">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="index.MainActivity" />
        </activity>
        <activity
            android:name=".advance.AdvanceActivity"
            android:label="@string/advance">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="index.MainActivity" />
        </activity>
        <activity
            android:name=".index.MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".grid_view.GridviewActivity" />
        <activity android:name=".list_view.ListviewActivity" />
        <activity android:name=".tab_view.TabviewActivity"></activity>
    </application>

</manifest>